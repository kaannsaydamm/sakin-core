{
  "Database": {
    "Host": "localhost",
    "Username": "postgres",
    "Password": "TODO_SET_VIA_USER_SECRETS",
    "Database": "ingest_db_dev",
    "Port": 5432
  },
  "Kafka": {
    "BootstrapServers": "localhost:29092",
    "Producer": {
      "BatchSize": 10,
      "LingerMs": 5,
      "CompressionType": "None"
    },
    "Consumer": {
      "AutoOffsetReset": "Earliest",
      "MaxPollRecords": 10
    }
  },
  "Ingestion": {
    "BatchSize": 10,
    "FlushIntervalSeconds": 5,
    "EnableDeduplication": true,
    "EnableEnrichment": false
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Sakin.Ingest": "Trace",
      "Sakin.Ingest.Services": "Debug",
      "Sakin.Ingest.Processors": "Debug",
      "Sakin.Ingest.Sources": "Debug",
      "Sakin.Ingest.Sinks": "Debug",
      "Microsoft.Hosting.Lifetime": "Information",
      "System": "Information"
    }
  }
}
