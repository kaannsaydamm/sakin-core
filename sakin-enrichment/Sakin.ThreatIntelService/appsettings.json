{
  "Kafka": {
    "BootstrapServers": "localhost:9092",
    "ClientId": "threat-intel-worker",
    "ThreatIntelConsumerGroup": "threat-intel-worker"
  },
  "ThreatIntel": {
    "Enabled": true,
    "Providers": [
      {
        "Type": "OTX",
        "Enabled": true,
        "ApiKey": "",
        "BaseUrl": "https://otx.alienvault.com/api/v1",
        "DailyQuota": 600
      },
      {
        "Type": "AbuseIPDB",
        "Enabled": true,
        "ApiKey": "",
        "BaseUrl": "https://api.abuseipdb.com/api/v2",
        "DailyQuota": 1000
      }
    ],
    "MaliciousScoreThreshold": 80,
    "MaliciousCacheTtlDays": 7,
    "CleanCacheTtlHours": 1,
    "NotFoundCacheTtlHours": 24,
    "LookupTopic": "ti-lookup-queue"
  },
  "Redis": {
    "ConnectionString": "localhost:6379"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "Sakin.ThreatIntelService": "Information"
    }
  }
}
